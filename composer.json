{
    "name": "catrobat/catroweb",
    "type": "project",
    "license": "MIT",
    "description": "Catrobats' Share Community Platform",
    "require": {
        "php": ">=8.1",
        "ext-SimpleXML": "*",
        "ext-gd": "*",
        "ext-imagick": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pcntl": "*",
        "ext-posix": "*",
        "ext-zip": "*",
        "beberlei/doctrineextensions": "^1.3",
        "catrobat/capi": "1.1.17",
        "codercat/jwk-to-pem": "^1.1",
        "doctrine/annotations": "^1.13",
        "doctrine/dbal": "^3.3",
        "doctrine/doctrine-bundle": "^2.6",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.12",
        "eightpoints/guzzle-bundle": "^8.3",
        "friendsofsymfony/elastica-bundle": "^6.1",
        "gesdinet/jwt-refresh-token-bundle": "^1.0",
        "google/apiclient": "^2.12",
        "google/cloud-translate": "^1.12",
        "hwi/oauth-bundle": "dev-master",
        "incenteev/composer-parameter-handler": "^2.1",
        "lexik/jwt-authentication-bundle": "^2.15",
        "php-http/guzzle6-adapter": "^2.0",
        "php-http/httplug-bundle": "^1.26",
        "php-http/message": "^1.13",
        "ramsey/uuid": "^4.2",
        "ramsey/uuid-doctrine": "^2.0",
        "rosell-dk/webp-convert": "^2.9",
        "sonata-project/admin-bundle": "^4.11",
        "sonata-project/doctrine-orm-admin-bundle": "^4.2",
        "sonata-project/exporter": "^3.0",
        "sonata-project/twig-extensions": "^2.0",
        "sonata-project/user-bundle": "^5.1.0",
        "symfony/acl-bundle": "^2.3",
        "symfony/asset": "6.2.*",
        "symfony/cache": "6.2.*",
        "symfony/console": "6.2.*",
        "symfony/dotenv": "6.2.*",
        "symfony/flex": "^2.1",
        "symfony/framework-bundle": "6.2.*",
        "symfony/intl": "6.2.*",
        "symfony/mailer": "6.2.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/polyfill-iconv": "^1.23",
        "symfony/process": "6.2.*",
        "symfony/routing": "6.2.*",
        "symfony/translation": "6.2.*",
        "symfony/twig-bundle": "6.2.*",
        "symfony/validator": "6.2.*",
        "symfony/webpack-encore-bundle": "^1.14",
        "symfony/yaml": "6.2.*",
        "symfonycasts/reset-password-bundle": "^1.13",
        "symfonycasts/verify-email-bundle": "^1.10",
        "twig/extra-bundle": "^3.4",
        "twig/twig": "^3.4"
    },
    "require-dev": {
        "behat/behat": "^3.10",
        "behat/mink": "^1.10",
        "deployer/deployer": "v8.x-dev",
        "dg/bypass-finals": "^1.3",
        "dmore/behat-chrome-extension": "^1.4",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "dvdoug/behat-code-coverage": "^5.2",
        "friends-of-behat/symfony-extension": "^2.3",
        "friendsofphp/php-cs-fixer": "^3.11",
        "phpstan/phpstan": "^1.8.5",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpstan/phpstan-phpunit": "^1.1",
        "phpstan/phpstan-symfony": "^1.2",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.15.0",
        "symfony/browser-kit": "6.2.*",
        "symfony/http-client": "6.2.*",
        "symfony/maker-bundle": "^1.45",
        "symfony/phpunit-bridge": "6.2.*",
        "symfony/stopwatch": "6.2.*",
        "symfony/web-profiler-bundle": "6.2.*",
        "vimeo/psalm": "^5.4.0",
        "wapmorgan/php-deprecation-detector": "^2.0.17"
    },
    "config": {
        "platform": {
            "php": "8.1",
            "ext-curl": "8.1",
            "ext-gd": "8.1",
            "ext-imagick": "3.4.4",
            "ext-json": "1.7.0",
            "ext-mbstring": "8.1",
            "ext-pcntl": "8.1",
            "ext-posix": "8.1",
            "ext-zip": "8.1"
        },
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "fix": [
            "bin/php-cs-fixer fix --allow-risky=yes --verbose --format=txt"
        ]
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.2.*"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Catrobat/Catroweb-API"
        }
    ]
}
