{
  "name": "catrobat/catroweb",
  "type": "project",
  "license": "MIT",
  "description": "Catrobat's Share Community Platform",
  "require": {
    "php": ">=8.3",
    "ext-SimpleXML": "*",
    "ext-gd": "*",
    "ext-imagick": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-pcntl": "*",
    "ext-posix": "*",
    "ext-zip": "*",
    "beberlei/doctrineextensions": "1.5.*",
    "bugsnag/bugsnag-symfony": "^1.14",
    "catrobat/capi": "1.4.*",
    "codercat/jwk-to-pem": "1.1.*",
    "doctrine/annotations": "2.0.*",
    "doctrine/dbal": "3.8.*",
    "doctrine/doctrine-bundle": "2.12.*",
    "doctrine/doctrine-migrations-bundle": "3.3.*",
    "doctrine/orm": "3.1.*",
    "eightpoints/guzzle-bundle": "8.5.*",
    "friendsofsymfony/elastica-bundle": "6.3.*",
    "gesdinet/jwt-refresh-token-bundle": "1.3.*",
    "google/apiclient": "2.16.*",
    "google/cloud-translate": "1.17.*",
    "http-interop/http-factory-guzzle": "*",
    "hwi/oauth-bundle": "2.2.*",
    "incenteev/composer-parameter-handler": "2.2.*",
    "lexik/jwt-authentication-bundle": "3.0.*",
    "php-http/guzzle7-adapter": "1.0.*",
    "php-http/httplug-bundle": "1.33.*",
    "php-http/message": "1.16.*",
    "ramsey/uuid": "4.7.*",
    "ramsey/uuid-doctrine": "2.0.*",
    "rosell-dk/webp-convert": "2.9.*",
    "sonata-project/admin-bundle": "4.30.*",
    "sonata-project/doctrine-orm-admin-bundle": "4.17.*",
    "sonata-project/exporter": "3.3.*",
    "sonata-project/twig-extensions": "2.4.*",
    "sonata-project/user-bundle": "5.12.*",
    "symfony/acl-bundle": "2.3.*",
    "symfony/asset": "6.4.*",
    "symfony/brevo-mailer": "6.4.*",
    "symfony/cache": "6.4.*",
    "symfony/console": "6.4.*",
    "symfony/dotenv": "6.4.*",
    "symfony/flex": "2.4.*",
    "symfony/framework-bundle": "6.4.*",
    "symfony/intl": "6.4.*",
    "symfony/mailer": "6.4.*",
    "symfony/monolog-bundle": "3.10.*",
    "symfony/polyfill-iconv": "1.28.*",
    "symfony/process": "6.4.*",
    "symfony/routing": "6.4.*",
    "symfony/stimulus-bundle": "2.13.*",
    "symfony/translation": "6.4.*",
    "symfony/twig-bundle": "6.4.*",
    "symfony/validator": "6.4.*",
    "symfony/webpack-encore-bundle": "2.1.*",
    "symfony/yaml": "6.4.*",
    "symfonycasts/reset-password-bundle": "1.21.*",
    "symfonycasts/verify-email-bundle": "1.17.*",
    "twig/extra-bundle": "3.10.*",
    "twig/twig": "3.10.*"
  },
  "require-dev": {
    "behat/behat": "3.14.*",
    "behat/mink": "1.11.*",
    "deployer/deployer": "v7.4.*",
    "dmore/behat-chrome-extension": "1.4.*",
    "dmore/chrome-mink-driver": "2.9.*",
    "doctrine/doctrine-fixtures-bundle": "3.6.*",
    "dvdoug/behat-code-coverage": "5.3.*",
    "friends-of-behat/symfony-extension": "2.5.*",
    "friends-of-behat/mink-extension": "v2.7.5",
    "friendsofphp/php-cs-fixer": "3.56.*",
    "phpstan/phpstan": "1.10.*",
    "phpstan/phpstan-doctrine": "1.3.*",
    "phpstan/phpstan-phpunit": "1.3.*",
    "phpstan/phpstan-symfony": "1.3.*",
    "phpunit/phpunit": "10.5.*",
    "rector/rector": "1.0.*",
    "symfony/browser-kit": "7.0.*",
    "symfony/http-client": "6.4.*",
    "symfony/maker-bundle": "1.58.*",
    "symfony/phpunit-bridge": "6.4.*",
    "symfony/stopwatch": "6.4.*",
    "symfony/web-profiler-bundle": "6.4.*",
    "vimeo/psalm": "5.23.*",
    "wapmorgan/php-deprecation-detector": "2.0.*",
    "textalk/websocket": "1.5.8.*"
  },
  "config": {
    "platform": {
      "php": "8.3",
      "ext-curl": "8.3",
      "ext-gd": "8.3",
      "ext-imagick": "3.4.4",
      "ext-json": "1.7.0",
      "ext-mbstring": "8.3",
      "ext-pcntl": "8.3",
      "ext-posix": "8.3",
      "ext-xml": "8.3",
      "ext-zip": "8.3"
    },
    "bin-dir": "bin",
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/flex": true,
      "php-http/discovery": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "fix": [
      "PHP_CS_FIXER_IGNORE_ENV=1 bin/php-cs-fixer fix --allow-risky=yes --verbose --format=txt"
    ]
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "6.4.*"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/Catrobat/Catroweb-API"
    }
  ]
}
