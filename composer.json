{
    "name": "catrobat/catroweb",
    "type": "project",
    "license": "MIT",
    "description": "Catrobats' Share Community Platform",
    "require": {
        "php": ">=7.4",
        "ext-SimpleXML": "*",
        "ext-gd": "*",
        "ext-imagick": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pcntl": "*",
        "ext-posix": "*",
        "ext-zip": "*",
        "beberlei/doctrineextensions": "^1.3",
        "catrobat/capi": "1.0.64",
        "codercat/jwk-to-pem": "^1.1",
        "doctrine/annotations": "^1.13",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "^2.7",
        "eightpoints/guzzle-bundle": "^8.2",
        "friendsofsymfony/elastica-bundle": "^6.1",
        "friendsofsymfony/jsrouting-bundle": "^2.7",
        "friendsofsymfony/rest-bundle": "^2.8",
        "gesdinet/jwt-refresh-token-bundle": "^1.0.0",
        "google/apiclient": "^2.11",
        "google/cloud-translate": "^1.12",
        "hwi/oauth-bundle": "^1.4",
        "incenteev/composer-parameter-handler": "^2.1",
        "lexik/jwt-authentication-bundle": "^2.13",
        "php-http/guzzle6-adapter": "^2.0",
        "php-http/httplug-bundle": "^1.23",
        "php-http/message": "^1.12",
        "rosell-dk/webp-convert": "^2.9",
        "sonata-project/admin-bundle": "^3.68|^4.0",
        "sonata-project/doctrine-orm-admin-bundle": "^3.18|^4.0",
        "sonata-project/user-bundle": "^4.5|^5.0",
        "symfony/acl-bundle": "^2.0",
        "symfony/asset": "4.4.*",
        "symfony/cache": "5.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "5.4.*",
        "symfony/flex": "^1.7",
        "symfony/intl": "5.4.*",
        "symfony/mailer": "5.4.*",
        "symfony/monolog-bundle": "^3.7",
        "symfony/polyfill-iconv": "^1.23",
        "symfony/process": "5.4.*",
        "symfony/routing": "4.4.*",
        "symfony/security": "4.4.*",
        "symfony/swiftmailer-bundle": "^3.5",
        "symfony/translation": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/webpack-encore-bundle": "^1.12",
        "symfony/yaml": "5.3.*",
        "symfonycasts/reset-password-bundle": "^1.11",
        "symfonycasts/verify-email-bundle": "^1.7",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "behat/behat": "^3.8",
        "behat/mink": "^1.8",
        "behat/mink-extension": "^2.3",
        "behat/symfony2-extension": "^2.1",
        "deployer/deployer": "^6.8",
        "deployer/recipes": "^6.2",
        "dg/bypass-finals": "^1.3",
        "dmore/behat-chrome-extension": "^1.3",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "dvdoug/behat-code-coverage": "^5.2",
        "friendsofphp/php-cs-fixer": "^3.1",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-doctrine": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpstan/phpstan-symfony": "^0.12",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "5.4.*",
        "symfony/maker-bundle": "^1.34",
        "symfony/phpunit-bridge": "6.0.*",
        "symfony/stopwatch": "4.4.*",
        "symfony/web-profiler-bundle": "5.0.*",
        "vimeo/psalm": "^4.10",
        "vipsoft/doctrine-data-fixtures-extension": "3.0.*@dev",
        "wapmorgan/php-deprecation-detector": "^2.0"
    },
    "config": {
        "platform": {
            "php": "7.4",
            "ext-gd": "7.4",
            "ext-imagick": "3.4.4",
            "ext-json": "1.7.0",
            "ext-mbstring": "7.4",
            "ext-pcntl": "7.4",
            "ext-posix": "7.4",
            "ext-zip": "1.15.4"
        },
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "fix": [
            "bin/php-cs-fixer fix --allow-risky=yes --verbose --format=txt"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Catrobat/Catroweb-API"
        }
    ]
}
