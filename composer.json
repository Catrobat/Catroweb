{
    "type": "project",
    "license": "proprietary",

    "require": {
        "php": ">=7.2",
        "ext-SimpleXML": "^7.1",
        "ext-gd": "^7.2",
        "ext-json": "^1.5",
        "doctrine/data-fixtures": "^1.3",
        "doctrine/doctrine-bundle": "^1.9",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "doctrine/doctrine-migrations-bundle": "^1.3",
        "doctrine/migrations": "^1.8",
        "doctrine/orm": "^2.6",
        "eightpoints/guzzle-bundle": "^7.3",
        "facebook/graph-sdk": "^5.6",
        "fr3d/ldap-bundle": "^3.0",
        "friendsofsymfony/jsrouting-bundle": "^2.2",
        "friendsofsymfony/rest-bundle": "^2.4",
        "friendsofsymfony/user-bundle": "^2.1",
        "geocoder-php/ip-info-provider": "^0.1.0",
        "google/apiclient": "^2.2",
        "incenteev/composer-parameter-handler": "^2.1",
        "knplabs/knp-paginator-bundle": "^2.8",
        "liip/theme-bundle": "dev-master",
        "php-http/message": "^1.7",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^5.2",
        "sonata-project/admin-bundle": "3.39",
        "sonata-project/doctrine-orm-admin-bundle": "^3.6",
        "sonata-project/user-bundle": "^4.2",
        "symfony/acl-bundle": "^1.0",
        "symfony/console": "*",
        "symfony/framework-bundle": "*",
        "symfony/monolog-bundle": "^3.3",
        "symfony/polyfill-iconv": "^1.8",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/yaml": "*",
        "twbs/bootstrap": "^4.1",
        "willdurand/geocoder-bundle": "^5.1",
        "ext-imagick": "*",
        "ext-posix": "*"
    },
    "require-dev": {
        "behat/behat": "^3.5",
        "behat/mink-extension": "^2.3",
        "behat/symfony2-extension": "^2.1",
        "bossa/phpspec2-expect": "^3.0",
        "deployer/deployer": "^6.3",
        "dmore/behat-chrome-extension": "^1.2",
        "leanphp/phpspec-code-coverage": "^4.2",
        "phpcheckstyle/phpcheckstyle": "^0.14.8",
        "phpspec/phpspec": "^4.0",
        "phpunit/phpunit": "^7.3",
        "symfony/dotenv": "*",
        "symfony/maker-bundle": "^1.6",
        "symfony/phpunit-bridge": "*",
        "vipsoft/doctrine-data-fixtures-extension": "3.0.*@dev",
        "wapmorgan/php-code-fixer": "dev-master"
    },

    "config": {
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },

    "autoload": {
        "psr-0": {
            "": "src/",
            "SymfonyStandard": "app/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },

    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },

    "scripts": {
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles"
        ]
    },

    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "3.4.*"
        },
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-var-dir": "var",
        "symfony-bin-dir": "bin",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
