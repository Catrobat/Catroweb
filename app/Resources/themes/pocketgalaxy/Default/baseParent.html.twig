<!DOCTYPE html>
<html prefix="og: http://ogp.me/ns#">
<head>
  <meta http-equiv="Content-Type" content="text/html; charset={{ _charset }}"/>
  <meta name="robots" content="noindex,nofollow"/>
  <meta name="viewport" content="width=device-width">
  <title>{% block title %}Galaxy Jam Website{% endblock %}</title>
  <link rel="shortcut icon" href="{{ asset('images/logo/favicon.png') }}" />

  <link rel="stylesheet" href="{{ asset('css/'~theme()~'/base.css') }}" media="screen"/>

  <script src=" {{  asset( getJavascriptPath('script.js') )}}"></script>
  
  <script>
    var configGA = { 
      'UaId' : 'UA-42270417-5',
      //'DebugOutput' : true,
    };
  </script>

  <script async src="{{ asset(getJavascriptPath('analytics.js')) }}"></script>

  {% block head %}
    <link rel="stylesheet" href="{{ asset('css/'~theme()~'/index.css') }}" media="screen"/>
  {% endblock %}
</head>
<body>

{% set overrideEventLink %}
  {% block eventLink %}{% endblock %}
{% endset %}
{% set overrideLogoImage %}
  {% block logoImage %}{% endblock %}
{% endset %}


<div id="wrapper">
  <div id="header-wrapper">
  <header>
    {% block header %}
      {% embed ':Default:header.html.twig' %}
        {% block eventLink %} {{ overrideEventLink }}{% endblock %}
        {% block logoImage %} {{ overrideLogoImage }}{% endblock %}
      {% endembed %}
    {% endblock %}
  </header>
  </div>
  <article>

      {% block body %}

          <div id="featuredPrograms">
              <h1>{{ "programs.featured"|trans({}, "catroweb") }}</h1>
              <div id="feature-slider" class="owl-carousel">
                  {% for feature in featured %}
                      <div><a href="{{ feature.url }}"><img src="{{ asset(feature.image) }}" /></a></div>
                  {% endfor %}
              </div>
          </div>

          <div id="submissions">
              <h1>{{ "programs.submitted"|trans({}, "catroweb") }}</h1>
              <div class="programs"></div>
          </div>

          <div id="sample">
              <h1>{{ "programs.sample"|trans({}, "catroweb") }}</h1>
              <div class="programs"></div>
          </div>

          <div id="related">
              <h1>{{ "programs.related"|trans({}, "catroweb") }}</h1>
              <div class="programs"></div>
          </div>

      {% endblock %}
  </article>
</div>

<div id="footer-wrapper">
<footer>
  {% block footer %}
      {% include ':Default:footer.html.twig' %}
  {% endblock %}
</footer>
</div>

<script>
  new Main('{{ path('search', {'q': 0 } ) }}');
</script>
{% block js %}{% endblock %}


</body>
</html>

