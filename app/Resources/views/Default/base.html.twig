<!DOCTYPE html>
<html prefix="og: http://ogp.me/ns#">
<head>
  <meta http-equiv="Content-Type" content="text/html; charset={{ _charset }}"/>
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <meta name="robots" content="noindex,nofollow"/>
  <meta name='viewport' content='width=device-width, user-scalable=no' />
  <title>Pocket Code Website {% block title %}{% endblock %}</title>
  <link rel="shortcut icon" href="{{ asset('images/logo/favicon.png') }}" />
  <link rel="stylesheet" href="{{ asset('css/'~theme()~'/base.css') }}" media="screen"/>

  <link rel="stylesheet" href="{{ asset('compiled/bootstrap/bootstrap.min.css') }}"/>
  <link rel="stylesheet" href="{{ asset('compiled/bootstrap/bootstrap_yeti.min.css') }}"/>


  {% block head %}{% endblock %}
</head>
<body>

<div id="wrapper">
  <header>
    {% block header %}
      {% include ':Default:header.html.twig' %}
    {% endblock %}
  </header>

  <article>
    {% block body %}{% endblock %}
  </article>
</div>

<footer>
  {% block footer %}
      {% include ':Default:footer.html.twig' %}
  {% endblock %}
</footer>

<script src=" {{  asset('compiled/bootstrap/jquery.min.js')}}">
</script>
<script src="{{  asset( getJavascriptPath('script.js') )}}">

</script>

<script>
new Main('{{ path('search', {'q': 0 } ) }}');
</script>
<script src=" {{  asset('compiled/bootstrap/bootstrap.min.js')}}"></script>

{% block js %}{% endblock %}

<script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
            (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
          m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-42270417-5', 'auto');
  ga('send', 'pageview');

</script>

</body>
</html>
