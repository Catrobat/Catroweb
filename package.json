{
  "name": "catroweb",
  "version": "3.20.34",
  "description": "Catroweb - The share communication platform for the Catrobat community",
  "scripts": {
    "test": "npm run test-js && npm run test-css && npm run test-php",
    "fix": "npm run fix-js && npm run fix-css && npm run fix-php",
    "test-js": "npx eslint assets/js/*.js assets/js/**/*.js webpack.config.js",
    "fix-js": "npx eslint assets/js/*.js assets/js/**/*.js webpack.config.js --fix",
    "test-css": "npx stylelint assets/**/*.scss assets/**/**/*.scss",
    "fix-css": "npx stylelint assets/**/*.scss assets/**/**/*.scss --fix",
    "test-asset": "npx prettier . --check",
    "fix-asset": "npx prettier . --write",
    "test-php": "php bin/php-cs-fixer fix --dry-run --allow-risky=yes --verbose --format=txt",
    "fix-php": "php bin/php-cs-fixer fix --allow-risky=yes --verbose --format=txt",
    "dev-server": "encore dev-server",
    "dev": "encore dev",
    "watch": "encore dev --watch",
    "build": "encore production --progress",
    "encore": "encore",
    "prod": "encore prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Catrobat/Catroweb.git"
  },
